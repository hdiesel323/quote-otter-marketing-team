# QuoteOtter AI Agent System - Environment Variables
# Copy this file to .env and fill in your values

# ===========================
# Application Configuration
# ===========================
NODE_ENV=production
PORT=3000
API_URL=http://w44okccwwsokwscks88kgswo.46.224.16.19.sslip.io

# ===========================
# QuoteOtter Platform
# ===========================
QUOTEOTTER_API_URL=http://w44okccwwsokwscks88kgswo.46.224.16.19.sslip.io
QUOTEOTTER_API_KEY=your-quoteotter-api-key-here

# ===========================
# PhoneRevealr Integration
# ===========================
PHONEREVEALR_API_KEY=your-phonerevealr-api-key-here
PHONEREVEALR_API_URL=https://phonerevealr.com/api
ENABLE_PHONEREVEALR=true

# ===========================
# AI Model Providers
# ===========================
# OpenAI (GPT-4, GPT-4 Turbo)
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic (Claude Sonnet 4)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Google AI (Gemini)
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Default model preference
DEFAULT_MODEL=claude-sonnet-4

# ===========================
# Database Configuration
# ===========================
DATABASE_URL=postgresql://postgres:your-secure-password@postgres:5432/quoteotter_agents
POSTGRES_PASSWORD=your-secure-postgres-password

# ===========================
# Redis Configuration
# ===========================
REDIS_URL=redis://:your-redis-password@redis:6379
REDIS_PASSWORD=your-secure-redis-password

# ===========================
# Feature Flags
# ===========================
ENABLE_FRAUD_DETECTION=true
ENABLE_ANALYTICS=true
ENABLE_LEAD_SCORING=true
ENABLE_PROVIDER_MATCHING=true

# ===========================
# Logging Configuration
# ===========================
LOG_LEVEL=info  # debug, info, warn, error
LOG_FORMAT=json  # json or text

# ===========================
# Security
# ===========================
JWT_SECRET=your-jwt-secret-here
API_RATE_LIMIT=100  # requests per minute
CORS_ORIGINS=http://w44okccwwsokwscks88kgswo.46.224.16.19.sslip.io,https://quoteotter.com

# ===========================
# Email Configuration (Optional)
# ===========================
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
SMTP_FROM=noreply@quoteotter.com

# ===========================
# Monitoring (Optional)
# ===========================
GRAFANA_PASSWORD=your-grafana-password
PROMETHEUS_ENABLED=false

# ===========================
# Lead Processing Configuration
# ===========================
# Minimum quality score to auto-route (0-100)
MIN_QUALITY_SCORE=60

# Lead routing strategy
ROUTING_STRATEGY=performance_based  # performance_based, round_robin, or load_balanced

# Maximum providers per lead
MAX_PROVIDERS_PER_LEAD=5

# Lead exclusivity window (hours)
EXCLUSIVITY_WINDOW_HOURS=4

# ===========================
# Fraud Detection Thresholds
# ===========================
# Minimum risk score to approve (0-100, higher = better)
MIN_RISK_SCORE=70

# Auto-reject VoIP leads from high-risk carriers
AUTO_REJECT_HIGH_RISK_VOIP=true

# Maximum duplicate submissions within 30 days
MAX_DUPLICATE_SUBMISSIONS=3

# Geographic distance tolerance (miles)
MAX_IP_ZIP_DISTANCE=500

# ===========================
# Coolify Configuration
# ===========================
# These are managed by Coolify - don't edit manually
COOLIFY_APP_ID=
COOLIFY_DEPLOYMENT_UUID=
COOLIFY_BRANCH=main
COOLIFY_COMMIT_SHA=
